<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal">

    <android.support.v7.widget.CardView
        android:id="@+id/cardview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        card_view:cardCornerRadius="10dp"
        card_view:cardBackgroundColor="@color/main_color_grey_300"
        card_view:cardElevation="@dimen/cardview_compat_inset_shadow"
        android:layout_margin="10dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">
        <LinearLayout
            android:minHeight="100dp"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:paddingTop="10dp"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_marginLeft="30dp"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_gravity="center"
                    android:background="@color/material_light_blue_300"
                    android:gravity="right"
                    />
                <TextView

                    android:textColor="@color/main_color_grey_800"
                    android:textSize="14dp"
                    android:paddingLeft="5dp"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:layout_weight="2"
                    android:text="고객코드"/>

                <TextView

                    android:id="@+id/customerCode"
                    android:textColor="@color/main_color_grey_800"
                    android:textSize="14dp"

                    android:layout_weight="2"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:text="11021"/>


                <TextView
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_gravity="center"
                    android:background="@color/material_light_blue_300"
                    android:gravity="left"
                    />

                <TextView
                    android:id="@+id/endclick"
                    android:textColor="@color/main_color_grey_800"
                    android:textSize="14dp"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:layout_weight="2"
                    android:paddingLeft="5dp"
                    android:text="고객명"/>

                <TextView
                    android:id="@+id/customerName"
                    android:textColor="@color/main_color_grey_800"
                    android:textSize="14dp"
                    android:layout_weight="2"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:text="동작슈퍼"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="0.5"
                    android:layout_height="2dp" />

            </LinearLayout>
            <LinearLayout
                android:paddingTop="10dp"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:visibility="invisible"
                    android:layout_marginLeft="30dp"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_gravity="center"
                    android:background="@color/material_light_blue_300"
                    android:gravity="right"
                    />
                <TextView
                    android:visibility="invisible"
                    android:textColor="@color/main_color_grey_800"
                    android:textSize="14dp"
                    android:paddingLeft="5dp"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:layout_weight="2"
                    android:text="고객코드"/>

                <TextView
                    android:visibility="invisible"
                    android:textColor="@color/main_color_grey_800"
                    android:textSize="14dp"

                    android:layout_weight="2"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:text="동작슈퍼"/>


                <TextView
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_gravity="center"
                    android:background="@color/material_light_blue_300"
                    android:gravity="left"
                    />

                <TextView

                    android:textColor="@color/main_color_grey_800"
                    android:textSize="14dp"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:layout_weight="2"
                    android:paddingLeft="5dp"
                    android:text="대표자"/>

                <TextView
                    android:id="@+id/chiefName"
                    android:textColor="@color/main_color_grey_800"
                    android:textSize="14dp"
                    android:layout_weight="2"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:text="최경호"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="0.5"
                    android:layout_height="2dp" />

            </LinearLayout>
            <LinearLayout
                android:paddingTop="10dp"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_marginLeft="30dp"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_gravity="center"
                    android:background="@color/material_light_blue_300"
                    android:gravity="right"
                    />
                <TextView

                    android:textColor="@color/main_color_grey_800"
                    android:textSize="14dp"
                    android:paddingLeft="5dp"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:layout_weight="1"
                    android:text="주소"/>

                <TextView
                    android:id="@+id/customerAddress"
                    android:textColor="@color/main_color_grey_800"
                    android:textSize="14dp"

                    android:layout_weight="3.5"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:text="서울특별시 동작구 사당동"/>



            </LinearLayout>

        <LinearLayout
            android:background="@color/white"
            android:layout_margin="30dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

        <LinearLayout
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_marginLeft="30dp"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_gravity="center"
                    android:background="@color/material_light_blue_300"
                    android:gravity="right"
                    />
                <TextView

                    android:textColor="@color/main_color_grey_800"
                    android:textSize="14dp"
                    android:paddingLeft="5dp"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:layout_weight="1"
                    android:text="담당자"/>

                <TextView

                    android:textColor="@color/main_color_grey_800"
                    android:textSize="14dp"

                    android:layout_weight="3.5"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:text="서울특별시 동작구 사당동"/>



            </LinearLayout>
            <LinearLayout
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_marginLeft="30dp"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_gravity="center"
                    android:background="@color/material_light_blue_300"
                    android:gravity="right"
                    />
                <TextView

                    android:textColor="@color/main_color_grey_800"
                    android:textSize="14dp"
                    android:paddingLeft="5dp"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:layout_weight="1"
                    android:text="연락처"/>

                <TextView

                    android:textColor="@color/main_color_grey_800"
                    android:textSize="14dp"

                    android:layout_weight="3.5"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:text="서울특별시 동작구 사당동"/>



            </LinearLayout>
            <LinearLayout
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_marginLeft="30dp"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_gravity="center"
                    android:background="@color/material_light_blue_300"
                    android:gravity="right"
                    />
                <TextView

                    android:textColor="@color/main_color_grey_800"
                    android:textSize="14dp"
                    android:paddingLeft="5dp"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:layout_weight="1"
                    android:text="이메일"/>

                <TextView

                    android:textColor="@color/main_color_grey_800"
                    android:textSize="14dp"

                    android:layout_weight="3.5"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:text="서울특별시 동작구 사당동"/>



            </LinearLayout>


</LinearLayout>


        </LinearLayout>



    </android.support.v7.widget.CardView>

</LinearLayout>